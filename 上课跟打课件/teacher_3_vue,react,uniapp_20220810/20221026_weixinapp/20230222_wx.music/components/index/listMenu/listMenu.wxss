/* components/index/listMenu/listMenu.wxss */

.listMenu{
  width:95%;
  margin: 20rpx;
  display: flex;
}
.left{
  width: 30%;
}
.right{
  box-sizing: border-box;
  width: 70%;
  height: 260rpx;
  color:#919191;
  background-color: #333;
  padding:20rpx 40rpx;
  font-size: 35rpx;
}
.listImage{
  width: 100%;
  height: 300rpx;
}

.listTitle{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin: 20rpx 0;
}